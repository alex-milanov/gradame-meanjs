doctype html
html(lang="en")
  head
    title= title
    link(rel='stylesheet', href='/lib/bootstrap/dist/css/bootstrap.min.css')
    link(rel='stylesheet', href='/lib/font-awesome/css/font-awesome.min.css')
    link(rel='stylesheet', href='/lib/angular-image-crop/image-crop-styles.css')
    
    link(rel='stylesheet', href='/css/style.css')
  body
    header.navbar.navbar-default.navbar-fixed-top(role='navigation',ng-include="'views/partials/header.html'")
    section.container
      section.content
        block content
    footer(ng-include="'views/partials/footer.html'")

    //- Scripts
    //- Other
    script(src="lib/lodash/dist/lodash.js")
    script(src="lib/q/q.js")
    //- jQuery
    script(src="lib/jquery/dist/jquery.js")
    script(src="lib/bootstrap/dist/js/bootstrap.min.js")
    script(src="https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=true&&libraries=places")
    //- Angular pre load
    script.
      FileAPI = {
          jsUrl: 'lib/ng-file-upload-shim/FileAPI.min.js',
          flashUrl: 'lib/ng-file-upload-shim/FileAPI.flash.swf'
      }
    script(src="lib/ng-file-upload-shim/angular-file-upload-shim.min.js")
    //- Angural
    script(src="lib/angular/angular.js")
    script(src="lib/angular-ui-router/release/angular-ui-router.js")
    script(src="lib/angular-resource/angular-resource.js")
    script(src="lib/angular-google-maps/dist/angular-google-maps.js")
    script(src="lib/ngstorage/ngStorage.js")
    script(src="lib/angular-image-crop/image-crop.js")
    //- Angular file upload
    script(src="lib/ng-file-upload/angular-file-upload.min.js")
    script(src="lib/ng-file-upload/angular-file-upload.js")
    //- Angular file bindings
    script(src="lib/angular-file/src/angular-file.js")
    //- APP
    script(src="js/app.js")
    //- services
    script(src="js/services/token.js")
    script(src="js/services/user.js")
    script(src="js/services/auth.js")
    script(src="js/services/signal.js")
    script(src="js/services/maps.js")
    //- controllers
    script(src="js/controllers/signals.js")
    script(src="js/controllers/profile.js")
    script(src="js/controllers/signals.new.js")
    script(src="js/controllers/signals.view.js")
    script(src="js/controllers/nav.js")
    script(src="js/controllers/auth.js")
    //- directives
    script(src="js/directives/lastSignals.js")
